<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>

<fx:root maxHeight="500" maxWidth="500" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">


    <Label fx:id="titleView" text="null" wrapText="true">
      <padding>
         <Insets bottom="15.0" top="15.0" />
      </padding></Label>
    <Label fx:id="artistView" text="null" wrapText="true">
      <padding>
         <Insets bottom="15.0" />
      </padding></Label>
    <Label fx:id="subjectView" text="null" wrapText="true">
      <padding>
         <Insets bottom="15.0" />
      </padding></Label>
    <Label fx:id="dateView" text="null" wrapText="true">
      <padding>
         <Insets bottom="15.0" />
      </padding></Label>
   <Button fx:id="newWindow" onAction="#newWindow" text="New Window" />
    <ImageView fx:id="imageView" fitWidth="300.0" preserveRatio="true" />
   <Line endX="500.0" startX="-100.0" strokeWidth="3.0" />
</fx:root>
